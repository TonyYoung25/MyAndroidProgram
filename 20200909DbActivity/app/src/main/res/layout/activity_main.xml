<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">
    <Button
        android:id="@+id/sharePreferencesBtn"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="SharePreferencesButton"></Button>

    <Button
        android:id="@+id/fileBtn"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="FileButton"></Button>

    <Button
        android:id="@+id/sdcardBtn"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="SdcardButton"></Button>

    <Button
        android:id="@+id/dbBtn"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="dbButton"></Button>

    <Button
        android:id="@+id/httpBtn"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="httpButton"></Button>

</LinearLayout>